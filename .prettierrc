{
  "parser": "typescript",
  "printWidth": 120,
  "singleQuote": true,
  "bracketSpacing": true,
  "semi": true,
  "arrowParents": "avoid",
	"useTabs": false,
	"editor.formatOnSave": true,
  "jsxBracketSameLine": true,
  "jsxSingleQuote": true,
  "importOrder": [
		"<THIRD_PARTY_MODULES>",
		"(.*)lib/(.*)$",
		"^[./]"
	],
  "importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"importOrderParserPlugins": [
			"typescript",
			"jsx"
	]
}
